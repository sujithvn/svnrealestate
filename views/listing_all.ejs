<%- include('includes/_head.ejs') %>

<body>

  <!-- Navbar -->
  <%- include('includes/_navbar.ejs') %>

   <% const showcase_inner = {} %>
   <% showcase_inner.title = "Browse Our Properties" %>
   <% showcase_inner.detail = "Lorem ipsum dolor sit, amet consectetur adipisicing elit. Sunt, pariatur!" %>
   <%- include('includes/_showcase_inner.ejs', {showcase_inner: showcase_inner}) %>

  <!-- Breadcrumb -->
  <section id="bc" class="mt-3">
    <div class="container">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item">
            <a href="/">
              <i class="fas fa-home"></i> Home</a>
          </li>
          <li class="breadcrumb-item active"> Browse Listings</li>
        </ol>
      </nav>
    </div>
  </section>

  <!-- Listings -->
  <section id="listings" class="py-4">
    <div class="container">
      <div class="row">
        <% if (listings && listings.length > 0) { %>
          <% for(let listing of listings) { %> 
            <%- include('includes/_listing_single.ejs', {listing: listing}) %>
          <% } %> 
        <% } else { %>
          <h1>No listings found...</h1>
        <% } %>  
      </div>

      <%- include('includes/_pagination.ejs') %>
    </div>
  </section>

  
    <!-- Bottombar -->
    <%- include('includes/_bottombar.ejs') %>

  <!-- Footer -->
  <%- include('includes/_footer.ejs') %>