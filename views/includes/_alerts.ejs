<div id="message" class="container">
    <% for(let err of userrMessages) { %>
        <div class="alert alert-<%= alertType %> alert-dismissible text-center" role="alert">
            <button type="button" class="close" data-dismiss="alert">
                <span aria-hidden="true">&times;</span>
            </button>
            <%= err.msg %>
        </div>
    <% } %> 
</div>